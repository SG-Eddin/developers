  "main": "n/a",
  "scripts": {
    "watch:markdown": "gulp watch:markdown",
    "fetch:markdown": "git submodule init && git submodule update --remote",
    "copy:markdown": "gulp copy-markdown && npm run generate-readme",
    "update:markdown": "npm run fetch:markdown; npm run copy:markdown",
    "install:plugins": "cd ./plugins/gatsby-remark-copy-linked-files && npm install; npm run build && cd ../gatsby-remark-catch-relative-markdown-links && npm install && cd ../gatsby-plugin-segment-js && npm install; npm run build && cd ../../",
    "setup": "npm run clean; npm run update:markdown",
    "develop": "GRAPHQL_TOKEN=$(vault read -field=value secret/developersite/graphql_token) GATSBY_TARGET_ENV=stage gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve & ngrok http 9000 && kill $!",
    "build": "npm run setup; GRAPHQL_TOKEN=$(vault read -field=value secret/developersite/graphql_token) gatsby build",
    "build:pp": "GRAPHQL_TOKEN=$(vault read -field=value secret/developersite/graphql_token) GATSBY_TARGET_ENV=stage gatsby build --prefix-paths",
    "deploy:production": "GRAPHQL_TOKEN=$(vault read -field=value secret/developersite/graphql_token) GATSBY_TARGET_ENV=production gatsby build; gh-pages -b master -d public",
    "deploy:development": "GATSBY_TARGET_ENV=stage npm run build; npm run s3-upload developer.uport.space; npm run purge-cloudfront E1R1L12BCFEHU7",
    "deploy:next": "GRAPHQL_TOKEN=$(vault read -field=value secret/developersite/graphql_token) GATSBY_TARGET_ENV=stage gatsby build; npm run s3-upload next.uport.me; npm run purge-cloudfront E1H675FP388KHZ",
    "deploy:legacy": "GRAPHQL_TOKEN=$(vault read -field=value secret/developersite/graphql_token) GATSBY_TARGET_ENV=stage gatsby build; npm run s3-upload legacy.uport.me; npm run purge-cloudfront ES8XX0EL9EWAG ",
    "s3-upload": "node_modules/.bin/s3-deploy './public/**' --cwd './public/' --region us-west-2 --bucket",
    "purge-cloudfront": "node_modules/.bin/aws-cloudfront-invalidate",
    "generate-readme": "./scripts/generate-readme.js",
    "clean": "rm -rf public .cache node_modules/gh-pages/.cache content/public",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public,static .",
    "lint:md": "remark content/public/",
    "write-good": "write-good $(glob 'content/public/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "draft-release-notes": "GREN_GITHUB_TOKEN=$(vault read -field=value secret/mobile/release_token) gren release -d -l 1",
    "publish-release-notes": "GREN_GITHUB_TOKEN=$(vault read -field=value secret/mobile/release_token) gren release -l 1",
    "test": "jest"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      "preset-lint-markdown-style-guide",
      "preset-lint-consistent",
      "remark-lint-are-links-valid-alive",
      [
        "remark-lint-write-good",
        [
          "warn",
          {
            "passive": false
          }
        ]
      ],
      [
        "remark-lint-heading-length",
        [
          "warn",
          {
            "min": 3,
            "max": 12
          }
        ]
      ]
    ]
  }
}
