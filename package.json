  "main": "n/a",
  "scripts": {
    "clean:repos": "rm -rf repos/*",
    "watch:repos": "gulp watch:repos",
    "fetch": "gulp git:clone",
    "copy:markdown": "gulp copy-markdown; npm run generate-readme",
    "install:plugins": "cd ./plugins/gatsby-remark-copy-linked-files && npm install; npm run build && cd ../gatsby-remark-catch-relative-markdown-links && npm install && cd ../gatsby-plugin-segment-js && npm install; npm run build && cd ../../",
    "update:remote:docs": "npm run clean:repos; npm run fetch; npm run copy:markdown",
    "setup": "npm run install:plugins; npm run clean; npm run update:remote:docs",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve & ngrok http 9000 && kill $!",
    "build": "npm run setup; gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "deploy:production": "npm run build; gh-pages -b master -d public",
    "deploy:development": "npm run build; npm run s3-upload developer.uport.space; npm run purge-cloudfront E1R1L12BCFEHU7",
    "deploy:legacy": "npm run build; npm run s3-upload legacy.uport.me; npm run purge-cloudfront ES8XX0EL9EWAG ",
    "s3-upload": "node_modules/.bin/s3-deploy './public/**' --cwd './public/' --region us-west-2 --bucket",
    "purge-cloudfront": "node_modules/.bin/aws-cloudfront-invalidate",
    "generate-readme": "./scripts/generate-readme.js",
    "clean": "rm -rf public .cache node_modules/gh-pages/.cache content/public",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public,static .",
    "lint:md": "remark content/public/",
    "write-good": "write-good $(glob 'content/public/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
